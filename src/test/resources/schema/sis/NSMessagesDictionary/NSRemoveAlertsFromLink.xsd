								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="SchengenID" type="sisdt:SchengenIDType" maxOccurs="unbounded"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
