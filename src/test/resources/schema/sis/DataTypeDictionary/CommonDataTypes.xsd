			<xsd:documentation>Description: This field contains a nationality.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:CatalogEntryRD"/>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ST007_PersonRelatedRemarksType">
		<xsd:annotation>
			<xsd:documentation>Description:This field contains a remark related to a person. </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:CatalogEntryRD"/>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ST008_IdentificationMarkType">
		<xsd:annotation>
			<xsd:documentation>Description: This field contains a identification mark which is defined in a code table.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:CatalogEntryRD"/>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ST011_CategoryOfVehicleType">
		<xsd:annotation>
			<xsd:documentation>Description:This field contains one of the different categories of vehicles.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:CatalogEntryRD"/>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ST012_MakeOfVehicleType">
		<xsd:annotation>
