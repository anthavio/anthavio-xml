			<xsd:documentation>Description: This field contains the user/country which the request belongs to.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:CatalogEntryRD"/>
	</xsd:simpleType>
	<!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ST002_TypeOfRecordType">
