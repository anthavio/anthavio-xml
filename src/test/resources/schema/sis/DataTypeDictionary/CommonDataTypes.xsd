<xsd:schema xmlns:sisdt="http://www.europa.eu/schengen/sis/xsd/v1/types/common" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.europa.eu/schengen/sis/xsd/v1/types/common" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3.0 Candidate">
	<!-- ========================================================================= -->
	<!-- Relational Domains -->
	<!-- ========================================================================= -->
	<xsd:simpleType name="CatalogEntryRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates an entry from a code table.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="7"/>
			<xsd:pattern value="\d{4}\.\d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="DateTimeRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a date with a time.</xsd:documentation>
			<xsd:documentation>yyyy '-' mm '-' dd 'T' hh ':' mm ':' ss ('.' ms{3}) (zzzzzz)?</xsd:documentation>
			<xsd:documentation> yyyy is a four digit  that represents the year;The year is between 1800 and 2050)</xsd:documentation>
			<xsd:documentation>the remaining '-'s are separators between parts of the date portion</xsd:documentation>
			<xsd:documentation>the first mm is a two-digit numeral that represents the month</xsd:documentation>
			<xsd:documentation>dd is a two-digit numeral that represents the day</xsd:documentation>
			<xsd:documentation>'T' is a separator indicating that time-of-day follows</xsd:documentation>
			<xsd:documentation>hh is a two-digit numeral that represents the hour; '24' or more is not allowed</xsd:documentation>
			<xsd:documentation>':' is a separator between parts of the time-of-day portion</xsd:documentation>
			<xsd:documentation>the second mm is a two-digit numeral that represents the minute ; 60 or more is not allowed</xsd:documentation>
			<xsd:documentation>ss is a two-integer-digit numeral that represents the whole seconds; 60 or more is not allowed</xsd:documentation>
			<xsd:documentation>'.' ms{3} represents the fractional seconds. The number of Fractional Seconds will be 3</xsd:documentation>
			<xsd:documentation>zzzzzz (if present) represents the timezone</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:TextRD">
			<xsd:pattern value="(18\d\d|19\d\d|20[0-4]\d|2050)\-((0[1-9]|1[0-2])\-((0[1-9]|[1-2]\d|3[0-1])))T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]\.\d{3}(([\+\-]((0[0-9]|1[0-3]):[0-5][0-9]|14:00))|Z)?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="DateRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a date.</xsd:documentation>
			<xsd:documentation>Rule: format YYYYMMDD</xsd:documentation>
			<xsd:documentation>Rule: DD between [00...31]</xsd:documentation>
			<xsd:documentation>Rule: MM between [01...12]</xsd:documentation>
			<xsd:documentation>Rule: YYYY between [1800...2050]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:TextRD">
			<xsd:minLength value="8"/>
			<xsd:maxLength value="8"/>
			<xsd:pattern value="(18\d\d|19\d\d|20[0-4]\d|2050)((0[1-9]|1[0-2])((0[1-9]|[1-2]\d|3[0-1])))"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="DecimalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a numeric value, which includes an integer and a decimal values. A decimal seperator is allowed, the character to be used is a point.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="CapitalAlphaNumericRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a name without blanks.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
			<xsd:pattern value="[A-Z0-9]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ExtendedNameUnicodeTransformRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a transformed extended unicode boat name.</xsd:documentation>
			<xsd:documentation>Rule: Transformed version of ExtendedNameUnicode</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use: ".", "-", "/", "’", "(", ")", "?", ":", ",", "+", blank</xsd:documentation>
			<xsd:documentation>Rule: \n \r \t are not allowed.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="510"/>
			<xsd:pattern value="[A-Z0-9a-z\.\-/'()\?:,\+&#x00C0;-&#x00D6;&#x00D8;-&#x00F6;&#x00F8;-&#x00FF;&#x0100;-&#x0137;&#x0139;-&#x0148;&#x014A;-&#x017E;&#x0386;&#x0388;-&#x038A;&#x038C;&#x038E;-&#x03A1;&#x03A3;-&#x03CE;&#x0401;-&#x040C;&#x040E;-&#x044F;&#x0451;-&#x045C;&#x045E;&#x045F;&#x046A;&#x046B;&#x0474;&#x0475;&#x0490;&#x0491; ]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ExtendedNameUnicodeOriginalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed extended name unicode</xsd:documentation>
			<xsd:documentation>Rule: Transformed version of ExtendedNameUnicode</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use: ".", "-", "/", "’", "(", ")", "?", ":", ",", "+", blank</xsd:documentation>
			<xsd:documentation>Rule: \n \r \t are not allowed in the TextRD</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:ExtendedNameUnicodeTransformRD">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextNoBlankRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a name without blanks.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9\.\-/']+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="HexadecimalStringRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a hexadecimal string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-F0-9][A-F0-9]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextNoBlankTransformRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a transformed name.</xsd:documentation>
			<xsd:documentation>Rule: No Blanks are allowed</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use:  (?), (:) and (+)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="510"/>
			<xsd:pattern value="[a-zA-Z0-9\.\-/':\?\+]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextNoBlankOriginalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed name without blanks</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use:  (?), (:) and (+)</xsd:documentation>
			<xsd:documentation>Rule: No Blanks are allowed</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:NameFreeTextNoBlankTransformRD">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextTransformRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a transformed name.</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use:  (?), (:) and (+)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="510"/>
			<xsd:pattern value="[a-zA-Z0-9\.\-/':\?\+]([a-zA-Z0-9\.\-/':\?\+ ]*[a-zA-Z0-9\.\-/':\?\+])?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextOriginalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed name.</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use:  (?), (:) and (+)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:NameFreeTextTransformRD">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextSerialRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a serial name.</xsd:documentation>
			<xsd:documentation>Rule: "NAME_FREETEXT + comma + left and right bracket"</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9\.\-/',()]([a-zA-Z0-9\.\-/',() ]*[a-zA-Z0-9\.\-/',()])?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextSerialNoBlankRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a serial name without blanks</xsd:documentation>
			<xsd:documentation>Rule: "NAME_FREETEXT + comma + left and right bracket"</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9\.\-/',()]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextSerialTransformRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a transformed serial name.</xsd:documentation>
			<xsd:documentation>Rule: "NAME_FREETEXT + comma + left and right bracket"</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use:  (?), (:) and (+)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="510"/>
			<xsd:pattern value="[a-zA-Z0-9\.\-/',()\?:\+]([a-zA-Z0-9\.\-/',()\?:\+ ]*[a-zA-Z0-9\.\-/',()\?:\+])?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextSerialOriginalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed serial name.</xsd:documentation>
			<xsd:documentation>Rule: "NAME_FREETEXT + comma + left and right bracket"</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use:  (?), (:) and (+)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:NameFreeTextSerialTransformRD">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextSerialNoBlankTransformRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a transformed serial name.</xsd:documentation>
			<xsd:documentation>Rule: No Blanks are allowed</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="510"/>
			<xsd:pattern value="[a-zA-Z0-9\.\-/',()\?:\+]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameFreeTextSerialNoBlankOriginalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed serial name.</xsd:documentation>
			<xsd:documentation>Rule: "NAME_FREETEXT + comma + left and right bracket"</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use:  (?), (:) and (+)</xsd:documentation>
			<xsd:documentation>Rule: No Blanks are allowed</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:NameFreeTextSerialNoBlankTransformRD">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameUnicodeRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates unicode name.</xsd:documentation>
			<xsd:documentation>Rule:  Any information suitable to be input with Unicode by the participating countries. </xsd:documentation>
			<xsd:documentation>Rule: Greek and Cyrillic alphabet characters are also included</xsd:documentation>
			<xsd:documentation>Rule: including blanks</xsd:documentation>
			<xsd:documentation>Rule: "," , "." , "(", ")" , "+" , ":" ,"?" are forbidden </xsd:documentation>
			<xsd:documentation>Rule: \n \r \t are not allowed</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z0-9a-z\-/'&#x00C0;-&#x00D6;&#x00D8;-&#x00F6;&#x00F8;-&#x00FF;&#x0100;-&#x0137;&#x0139;-&#x0148;&#x014A;-&#x017E;&#x0386;&#x0388;-&#x038A;&#x038C;&#x038E;-&#x03A1;&#x03A3;-&#x03CE;&#x0401;-&#x040C;&#x040E;-&#x044F;&#x0451;-&#x045C;&#x045E;&#x045F;&#x046A;&#x046B;&#x0474;&#x0475;&#x0490;&#x0491; ]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameUnicodeTransformRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a transformed unicode name.</xsd:documentation>
			<xsd:documentation>Rule: Transformed version of NameUnicode</xsd:documentation>
			<xsd:documentation>Rule: Additional requirement for attribute "transformed" characters permit to use:  (?), (:) and (+)</xsd:documentation>
			<xsd:documentation>Rule: \n \r \t are not allowed</xsd:documentation>
			<xsd:documentation>Rule: "," , "." , "(", ")"  are forbidden </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="510"/>
			<xsd:pattern value="[A-Z0-9a-z\-/'\?:\+&#x00C0;-&#x00D6;&#x00D8;-&#x00F6;&#x00F8;-&#x00FF;&#x0100;-&#x0137;&#x0139;-&#x0148;&#x014A;-&#x017E;&#x0386;&#x0388;-&#x038A;&#x038C;&#x038E;-&#x03A1;&#x03A3;-&#x03CE;&#x0401;-&#x040C;&#x040E;-&#x044F;&#x0451;-&#x045C;&#x045E;&#x045F;&#x046A;&#x046B;&#x0474;&#x0475;&#x0490;&#x0491; ]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameUnicodeOriginalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed unicode name.</xsd:documentation>
			<xsd:documentation>Rule:  Any information suitable to be input with Unicode by the participating countries. </xsd:documentation>
			<xsd:documentation>Rule: Greek and Cyrillic alphabet characters are also included</xsd:documentation>
			<xsd:documentation>Rule: including blanks</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use:  (?), (:) and (+)</xsd:documentation>
			<xsd:documentation>Rule: \n \r \t are not allowed in the TextRD</xsd:documentation>
			<xsd:documentation>Rule: "," , "." , "(", ")"  are forbidden </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:NameUnicodeTransformRD">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameUnicodeDotTransformRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a transformed unicode name.</xsd:documentation>
			<xsd:documentation>Rule: Transformed version of NameUnicode</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use: (.)</xsd:documentation>
			<xsd:documentation>Rule: \n \r \t are not allowed in the TextRD</xsd:documentation>
			<xsd:documentation>Rule: ",", "(", ")", "?", ":" and "+" are forbidden </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="510"/>
			<xsd:pattern value="[A-Z0-9a-z\.\-/'&#x00C0;-&#x00D6;&#x00D8;-&#x00F6;&#x00F8;-&#x00FF;&#x0100;-&#x0137;&#x0139;-&#x0148;&#x014A;-&#x017E;&#x0386;&#x0388;-&#x038A;&#x038C;&#x038E;-&#x03A1;&#x03A3;-&#x03CE;&#x0401;-&#x040C;&#x040E;-&#x044F;&#x0451;-&#x045C;&#x045E;&#x045F;&#x046A;&#x046B;&#x0474;&#x0475;&#x0490;&#x0491; ]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameUnicodeDotOriginalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed unicode name.</xsd:documentation>
			<xsd:documentation>Rule:  Any information suitable to be input with Unicode by the participating countries. </xsd:documentation>
			<xsd:documentation>Rule: Greek and Cyrillic alphabet characters are also included</xsd:documentation>
			<xsd:documentation>Rule: including blanks</xsd:documentation>
			<xsd:documentation>Rule: Characters permit to use: (.)</xsd:documentation>
			<xsd:documentation>Rule: \n \r \t are not allowed in the TextRD</xsd:documentation>
			<xsd:documentation>Rule: ",", "(", ")", "?", ":" and "+"  are forbidden </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:NameUnicodeDotTransformRD">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameUnicodeAmnestyOriginalRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed unicode name. It is specified for amnistied Data without patterns</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:NameUnicodeAmnestyTransformRD">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NameUnicodeAmnestyTransformRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the original value of a transformed unicode name. It is specified for amnistied Data without patterns</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="510"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="NumRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates a numeric value.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:long">
			<xsd:minInclusive value="0" fixed="false"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="PictureRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates the picture data.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:base64Binary"/>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="TextRD">
		<xsd:annotation>
			<xsd:documentation>Description: This type indicates text.</xsd:documentation>
			<xsd:documentation>Rule: all characters form the unicode from the participating countries</xsd:documentation>
			<xsd:documentation>Rule: Greek and Cyrillic alphabet characters are also included</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4000"/>
			<xsd:pattern value="[A-Z0-9a-z\.\-/',()\?:\+&#x00C0;-&#x00D6;&#x00D8;-&#x00F6;&#x00F8;-&#x00FF;&#x0100;-&#x0137;&#x0139;-&#x0148;&#x014A;-&#x017E;&#x0386;&#x0388;-&#x038A;&#x038C;&#x038E;-&#x03A1;&#x03A3;-&#x03CE;&#x0401;-&#x040C;&#x040E;-&#x044F;&#x0451;-&#x045C;&#x045E;&#x045F;&#x046A;&#x046B;&#x0474;&#x0475;&#x0490;&#x0491; ]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="URLRD">
		<xsd:annotation>
			<xsd:documentation>Description: This simple contains the URL.  Used as technical link between http messages and attached FTP transferred documents.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:anyURI">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="250"/>
		</xsd:restriction>
	</xsd:simpleType>
