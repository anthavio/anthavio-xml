			<xsd:documentation>Description: This field contains the record type of an alert.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:CatalogEntryRD"/>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ST005_CategoryOfIdentityType">
		<xsd:annotation>
			<xsd:documentation>Description: This field contains one of the different categories of identities of persons.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="sisdt:CatalogEntryRD"/>
	</xsd:simpleType>
	<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<xsd:simpleType name="ST006_NationalityType">
